<div>
	<div class="container">

		{{#if token}}
		<h5>Welcome <span style='font-weight:800;''>{{name}}</span>, you are now successfully logged in!</h5>
		<p>Here are your friends from the resource server</p>
		
		<div class="row">
	        
		{{#each users}}
			<div class="col s12 m3">
	          <div class="card">
	            <div class="card-image">
	              <img src="{{this.image}}">
	              <span class="card-title">{{this.name}}</span>
	            </div>
	            <div class="card-content">
	              <p>{{this.address}}</p>
	              <p>{{this.city}}</p>
	              <p>{{this.phone}}</p>
	            </div>	            
	          </div>	     
          </div>
		{{/each}}
	    </div>	
		{{/if}}

	</div>
</div>